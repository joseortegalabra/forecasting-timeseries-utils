    "    grid = grid[['id','d','sales','item_id','dept_id','cat_id','store_id','state_id']]\n",
    "    grid = reduce_mem_usage(grid)\n",
    "    for col_name in ['cat_id', 'item_id', 'dept_id', 'store_id', 'store_id,cat_id', 'store_id,item_id', 'store_id,dept_id']:\n",
